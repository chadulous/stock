{
  "name": "stocklanguage",
  "displayName": "Stock Language Highlighting",
  "description": "",
  "publisher": "marina",
  "repository": "https://github.com/mavdotjs/stock",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "stockLanguage",
      "aliases": ["Stock", "stock"],
      "extensions": [".st", ".stock"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "stock",
      "scopeName": "source.stock",
      "path": "./syntaxes/stock.tmLanguage.json"
    }]
  }
}
